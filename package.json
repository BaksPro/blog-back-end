{
  "name": "node-web-starter",
  "version": "1.0.0",
  "description": "A skeleton for Node.js web applications",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel src -d build --copy-files",
    "serve": "npm run build && forever start build/index.js",
    "test": "mocha --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "http-status": "^0.2.2",
    "moment": "^2.14.1"
  },
  "author": "DK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvokov/node-web-starter.git"
  }
}
